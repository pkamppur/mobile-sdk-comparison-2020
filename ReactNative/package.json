{
  "name": "thehotnessrn",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test:e2e:ios:debug": "detox test -c ios",
    "test:e2e:ios:debug:build": "detox build -c ios",
    "lint": "eslint '{e2e,app,__test__}/**/*.ts*'",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --check '{e2e,app,__tests__}/**/*.{js,ts}*'",
    "prettier:fix": "npm run prettier -- --write"
  },
  "dependencies": {
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.5",
    "@react-navigation/stack": "^6.0.11",
    "react": "17.0.2",
    "react-native": "0.66.4",
    "react-native-gesture-handler": "^2.1.0",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.10.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/runtime": "^7.16.5",
    "@react-native-community/eslint-config": "^3.0.1",
    "@testing-library/jest-native": "^4.0.4",
    "@testing-library/react-native": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.3",
    "@types/react-native": "^0.66.10",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "babel-jest": "^27.4.5",
    "detox": "^19.4.1",
    "eslint": "^8.5.0",
    "eslint-plugin-react-native": "^4.0.0",
    "express": "^4.17.2",
    "faker": "^5.5.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.5",
    "jest-fetch-mock": "^3.0.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.5.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "jest-setup.js",
      "utils/*"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native|@react-native-community|react-navigation|@react-navigation/.*)"
    ],
    "setupFiles": [
      "<rootDir>/__tests__/jest-setup.js"
    ]
  }
}
